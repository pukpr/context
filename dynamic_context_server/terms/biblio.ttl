<>
	a cc:Work ,
	  owl:Ontology ;
	dc:creator "Bruce D'Arcus" ;
	dc:description "A vocabulary to describe a general data model for scholarly citations. \n     It covers three primary classes: events, agents, and bibliographic reference types. It \n     is designed to offer a solid general relational model for citation metadata, and also to\n     provide a specific superset of reference types in standard formats like BibTeX, RIS, and\n     Refer/Endnote." ;
	dc:rights "Copyright © 2006 Bruce D'Arcus" ;
	dc:title "Citation Oriented Bibliographic Vocabulary" ;
	dcterms:isVersionOf <biblio#> ;
	dcterms:issued "2006-02-10" ;
	vann:preferredNamespacePrefix "biblio" ;
	vann:preferredNamespaceUri "http://purl.org/net/biblio#" ;
	cc:license <http://creativecommons.org/licenses/by/2.0/> .

