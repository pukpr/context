#
@prefix ent: <http://entroplet.com/terms#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ent:conus
	a			ent:locale;
	ent:nickname		"conus";
	ent:property		ent:terrain_slopes;
	ent:characterization	ent:conus_terrain_slope_distro
	.

ent:terrain_slopes
	a			ent:property;
	ent:nickname		"terrain_slopes";
	ent:characterized_by	ent:pdf_distribution
	.

ent:pdf_distribution
	a	owl:Class.

ent:terrain_slope_distribution
	a	ent:pdf_distribution.

ent:conus_terrain_slope_distro
	a			ent:terrain_slope_distribution;
        ent:mean		0.039;
	ent:distribution_type	"besselk1";
	ent:min_extent		0.01;
	ent:max_extent		100.0;
	ent:interval_size	"0.1/decade";
	ent:x_axis		"rise/run";
	ent:title		"slope distribution over continental USA";
	ent:logarithmic		"true";
	ent:monte_carlo_model	"true"
	.

